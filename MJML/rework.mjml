      /*BEGIN HAM MENU STYLES*/
  		*[class].logo {
  			position:absolute;
  			top:0;
  			left:0;
  		}
  		*[class].menu {
  			background-image: url('http://joeldom.github.io/DSG/img/logo-blk-2.png') !important;
  			background-repeat: no-repeat;
  			background-color: #F1F1F1;
  			background-size: 140px 80px;
  			background-position: center;
  			width:100%;
  			height:80px;
  			margin:10px auto;
  			padding: 0 ;
  		}
  		*[class].menu ul {
  			max-height: 0;
  			overflow: hidden;
  			transition: max-height 0.25s linear;
  			margin: 0;
  			padding: 0;
  			text-align: right;
  		}
  		*[class].menu li {
  			visibility: hidden;
  			display: block;
  			border: none;
  			text-align: right;
  			background-color:#f1f1f1;
  			border-bottom:1px solid #c7c7c7;
  			border-left:1px solid #c7c7c7;
  		}
  		*[class].menu li a img.right{float: right;padding: 0px 10px 0 195px}
  		*[class].menu li { float:right;}
  		*[class].menu li:first-child { margin-top:90px !important;border-top:1px solid #c7c7c7;}
  		*[class].menu *[class].navbar-handle {
  			display: block;
  			background-image: url('http://joeldom.github.io/DSG/img/z_menu-grey.png') !important;
  			background-size: 26px 25px !important;
  		}
  		#navbar-checkbox:checked + *[class].menu ul {
  			max-height: 525px;
  			width: 100%
  		}
  		#navbar-checkbox:checked + *[class].menu li {
  			visibility: visible;
  			width: 100%;
  			text-align: right;
  		}
  		#navbar-checkbox:checked + *[class].menu *[class].navbar-handle,
  		#navbar-checkbox:checked + *[class].menu *[class].navbar-handle:after,
  		#navbar-checkbox:checked + *[class].menu *[class].navbar-handle:before {
  			background-image: url('http://joeldom.github.io/DSG/img/z_x-grey.png') !important; background-size: 26px 25px !important;
  		}
  		*[class].navbar-checkbox {
  			display: none;
  		}
  		*[class].navbar-handle {
  			display: none;
  			cursor: pointer;
  			padding: 0;
  			height: 25px;
  			width: 26px;
  			border-top: 0;
  		}
  		*[class].navbar-handle:before,
  		*[class].navbar-handle:after {
  			position: absolute;
  			content: ' ';
  		}
  		*[class].menu {
  			position: relative;
  		}
  		*[class].menu *[class].navbar-handle {
  			position: absolute;
  			top: 28px;
  			right: 15px;
  			z-index: 10;
  		}
  		/*END HAM MENU STYLES*/
